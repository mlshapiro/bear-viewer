{
  "name": "bear-viewer",
  "version": "0.0.1",
  "description": "Cross platform notes viewer for bear notes app",
  "repository": "https://github.com/mlshapiro/bear-viewer.git",
  "author": "Marc Shapiro",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.0.0",
    "babel-preset-env": "1.5.2",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "file-loader": "0.11.2",
    "live-server": "1.2.0",
    "style-loader": "^0.18.2",
    "stylus": "0.54.5",
    "vue-loader": "12.2.1",
    "vue-template-compiler": "2.3.4",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "scripts": {
    "style": "stylus www/index.styl",
    "serve": "live-server --open=\"www/index.html\" --watch=\"www\" --ignore=\"www/assets, **/*.json, **/*.css, **/*.styl\" ",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --content-base www/ --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "bootstrap-vue": "^0.16.1",
    "vue": "  2.3.4",
    "vue-router": "2.6.0"
  }
}
